{
  "manifest_version": 3,
  "name": "Pugpeep",
  "version": "1.0",
  "icons": {
    "16": "/icons/icon-16.png",
    "48": "/icons/icon-48.png",
    "128": "/icons/icon-128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/icons/icon-16.png",
      "48": "/icons/icon-48.png",
      "128": "/icons/icon-128.png"
    }
  },
  "permissions": [
    "scripting", 
    "tabs"
  ],
  "host_permissions": [
    "*://*/*"
  ]
}
